<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wayn.notify.dao.NotifyRecordDao">

    <resultMap type="com.wayn.notify.domain.NotifyRecord" id="NotifyRecordResult">
        <result property="id" column="id"/>
        <result property="notifyId" column="notifyId"/>
        <result property="reciveUserId" column="reciveUserId"/>
        <result property="isRead" column="isRead"/>
        <result property="readDate" column="readDate"/>
        <result property="createTime" column="createTime"/>
    </resultMap>

    <sql id="selectNotifyRecordVo">
        select id, notifyId, reciveUserId, isRead, readDate, createTime, from oa_notify_record
    </sql>

    <select id="selectNotifyRecordList" parameterType="NotifyRecord" resultMap="NotifyRecordResult">
        <include refid="selectNotifyRecordVo"/>
        <where>
            <if test="id != null ">and id = #{id}</if>
            <if test="notifyId != null ">and notifyId = #{notifyId}</if>
            <if test="reciveUserId != null  and reciveUserId != '' ">and reciveUserId = #{reciveUserId}</if>
            <if test="isRead != null ">and isRead = #{isRead}</if>
            <if test="readDate != null ">and readDate = #{readDate}</if>
            <if test="createTime != null ">and createTime = #{createTime}</if>
        </where>
    </select>

</mapper>
